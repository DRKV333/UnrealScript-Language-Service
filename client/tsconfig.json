{
	"compilerOptions": {
        "module": "commonjs",
        "target": "es2019",
        "lib": [
            "ES2019"
        ],
        "plugins": [
            {
                "name": "typescript-eslint-language-service"
            }
        ],
        "baseUrl": ".",
		"outDir": "out",
		"rootDir": "src",
		"sourceMap": true,
        "strict": true,
		"strictNullChecks": true
	},
	"include": ["src"],
    "exclude": [
		"node_modules",
		".vscode-test"
	],
}
