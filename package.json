{
    "name": "uc",
    "description": "UnrealScript Language support",
    "displayName": "UnrealScript",
    "version": "0.5.0",
    "author": {
        "name": "Eliot van Uytfanghe",
        "url": "https://EliotVU.com"
    },
    "publisher": "EliotVU",
    "homepage": "https://github.com/EliotVU/UnrealScript-Language-Service",
    "repository": {
        "type": "git",
        "url": "https://github.com/EliotVU/UnrealScript-Language-Service"
    },
    "license": "MIT",
    "capabilities": {
        "virtualWorkspaces": {
            "supported": false
        }
    },
    "categories": [
        "Programming Languages",
        "Linters"
    ],
    "keywords": [
        "UC",
        "UnrealScript",
        "IntelliSense"
    ],
    "engines": {
        "vscode": "^1.57.0"
    },
    "icon": "Icon.png",
    "dependencies": {
        "antlr4ts": "0.5.0-alpha.4"
    },
    "devDependencies": {
        "@tsconfig/recommended": "^1.0.1",
        "@types/node": "^17.0.12",
        "@typescript-eslint/eslint-plugin": "^5.10.1",
        "@typescript-eslint/parser": "^5.10.1",
        "antlr4ts-cli": "0.5.0-alpha.4",
        "copy-webpack-plugin": "^10.2.1",
        "eslint": "^8.7.0",
        "gulp": "^4.0.2",
        "js-yaml": "^4.1.0",
        "merge-options": "^3.0.4",
        "rimraf": "^3.0.2",
        "ts-loader": "^9.2.6",
        "ts-node": "^10.4.0",
        "tsconfig-paths-webpack-plugin": "^3.5.2",
        "typescript": "^4.5.5",
        "typescript-eslint-language-service": "^5.0.0",
        "webpack": "^5.67.0",
        "webpack-cli": "^4.9.2"
    },
    "scripts": {
        "vscode:prepublish": "cd client && npm run update-vscode && cd .. && npm run build",
        "build": "npx gulp buildSyntax && npm run compile:grammar && npm run compile:preprocessor && npm run clean && npx webpack --mode production --config ./webpack.config.js",
        "compile": "npx webpack --mode development --config ./webpack.config.js",
        "compile:grammar": "npx antlr4ts -visitor -no-listener grammars/UCLexer.g4 -o server/src/UC/antlr/generated && npx antlr4ts -visitor -no-listener grammars/UCParser.g4 -o server/src/UC/antlr/generated",
        "compile:preprocessor": "npx antlr4ts -visitor -no-listener grammars/UCPreprocessorParser.g4 -o server/src/UC/antlr/generated",
        "watch": "(set NODE_ENV=development) & start gulp & npx webpack --mode development --config ./webpack.config.js --watch",
        "clean": "rimraf out",
        "postinstall": "cd client && npm install && cd ../server && npm install && cd ..",
        "test": "cd server && npm test",
        "test-live": "cd server && npm run test-live"
    },
    "activationEvents": [
        "onLanguage:unrealscript",
        "workspaceContains:**/*.upkg"
    ],
    "main": "out/extension.js",
    "extensionKind": [
        "workspace"
    ],
    "configurationDefaults": {
        "[unrealscript]": {
            "editor.semanticHighlighting.enabled": true
        }
    },
    "contributes": {
        "configuration": {
            "type": "object",
            "title": "UnrealScript",
            "properties": {
                "unrealscript.trace.server": {
                    "scope": "window",
                    "type": "string",
                    "enum": [
                        "off",
                        "messages",
                        "verbose"
                    ],
                    "default": "off",
                    "description": "Traces the communication between VS Code and the language server."
                },
                "unrealscript.generation": {
                    "scope": "resource",
                    "type": "string",
                    "enum": [
                        "1",
                        "2",
                        "3"
                    ],
                    "default": "3",
                    "description": "The UnrealScript generation your project is working with.."
                },
                "unrealscript.licensee": {
                    "scope": "resource",
                    "type": "string",
                    "enum": [
                        "Epic",
                        "XCom"
                    ],
                    "default": "Epic",
                    "description": "The licensee's UnrealScript edition your project is working with. Changing this value requires a restart."
                },
                "unrealscript.indexAllDocuments": {
                    "scope": "window",
                    "type": "boolean",
                    "default": false
                },
                "unrealscript.analyzeDocuments": {
                    "scope": "window",
                    "type": "string",
                    "enum": [
                        "None",
                        "OnlyActive",
                        "All"
                    ],
                    "description": "Analyzes any active document, or all documents, and report back \"problems\" if any.",
                    "default": "OnlyActive"
                },
                "unrealscript.checkTypes": {
                    "scope": "window",
                    "type": "boolean",
                    "description": "Checks and reports if an expression's type is a valid one. e.g. assignments and passed arguments.",
                    "default": false
                },
                "unrealscript.macroSymbols": {
                    "scope": "window",
                    "type": "object",
                    "description": "A map of predefined preprocessor symbols.",
                    "default": {
                        "debug": ""
                    }
                },
                "unrealscript.intrinsicSymbols": {
                    "scope": "window",
                    "type": "object",
                    "description": "A map of intrinsic(C++) symbols that have no reflected source code.",
                    "default": {
                        "Engine.Model": {
                            "type": "class",
                            "extends": "Object"
                        },
                        "Engine.Mesh": {
                            "type": "class",
                            "extends": "Object"
                        },
                        "Engine.MeshInstance": {
                            "type": "class",
                            "extends": "Object"
                        },
                        "Engine.MeshAnimation": {
                            "type": "class",
                            "extends": "Object"
                        },
                        "Engine.StaticMesh": {
                            "type": "class",
                            "extends": "Actor"
                        },
                        "Engine.StaticMeshInstance": {
                            "type": "class",
                            "extends": "Actor"
                        },
                        "Engine.ConvexVolume": {
                            "type": "class",
                            "extends": "Actor"
                        },
                        "Engine.Level": {
                            "type": "class",
                            "extends": "Object"
                        },
                        "Engine.NetConnection": {
                            "type": "class",
                            "extends": "Object"
                        },
                        "Engine.Viewport": {
                            "type": "class",
                            "extends": "Object"
                        },
                        "Engine.Primitive": {
                            "type": "class",
                            "extends": "Object"
                        },
                        "Engine.Client": {
                            "type": "class",
                            "extends": "Object"
                        },
                        "Engine.AudioSubsystem": {
                            "type": "class",
                            "extends": "Subsystem"
                        },
                        "Engine.RenderDevice": {
                            "type": "class",
                            "extends": "Object"
                        }
                    }
                }
            }
        },
        "languages": [
            {
                "id": "unrealscript",
                "aliases": [
                    "UnrealScript",
                    "unrealscript"
                ],
                "extensions": [
                    ".uc",
                    ".uci"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "unrealscript",
                "scopeName": "source.uc",
                "path": "./syntaxes/UnrealScript.tmLanguage.json",
                "embeddedLanguages": {
                    "meta.embedded.block.cpp.uc": "cpp"
                }
            }
        ],
        "codeActions": [
            {
                "languages": [
                    "unrealscript"
                ],
                "actions": [
                    {
                        "kind": "refactor.extract.class",
                        "title": "Generate Class",
                        "description": ""
                    }
                ]
            }
        ]
    }
}
